<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>QTD Admin</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/Include/Admin/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/Include/Admin/vendors/base/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- plugin css for this page -->
    <link rel="stylesheet" href="~/Include/Admin/vendors/datatables.net-bs4/dataTables.bootstrap4.css">
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="~/Include/Admin/css/style.css">
    <!-- endinject -->
    <link rel="shortcut icon" href="~/Include/Admin/images/favicon.png" />
    @RenderSection("Scripts", false)

    <script src="~/Include/Web/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <script src="~/Include/Web/Scripts/jquery.validate.min.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/DevExtremeBundle")
    @Scripts.Render("~/Scripts/DevExtremeBundle")
</head>
<body>
    <div class="container-scroller">
        <!-- partial:partials/_navbar.html -->
        @Html.Partial("~/Areas/Admin/Views/Shared/_Nav.cshtml")
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            @Html.Partial("~/Areas/Admin/Views/Shared/_SideBar.cshtml")
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">

                    @RenderBody()
                </div>
                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                @Html.Partial("~/Areas/Admin/Views/Shared/_Footer.cshtml")
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/Include/Admin/vendors/base/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <script src="~/Include/Admin/vendors/chart.js/Chart.min.js"></script>
    <script src="~/Include/Admin/vendors/datatables.net/jquery.dataTables.js"></script>
    <script src="~/Include/Admin/vendors/datatables.net-bs4/dataTables.bootstrap4.js"></script>
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="~/Include/Admin/js/off-canvas.js"></script>
    <script src="~/Include/Admin/js/hoverable-collapse.js"></script>
    <script src="~/Include/Admin/js/template.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="~/Include/Admin/js/dashboard.js"></script>
    <script src="~/Include/Admin/js/data-table.js"></script>
    <script src="~/Include/Admin/js/jquery.dataTables.js"></script>
    <script src="~/Include/Admin/js/dataTables.bootstrap4.js"></script>
    <!-- End custom js for this page-->


</body>

</html>

